<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <!-- Separate user emails with commas (no spaces). -->
        <!-- To allow any user email, leave IncludedUserEmails blank. -->
        <add key="IncludedUserEmails" value="" />
        
        <!-- The ActiveCollabCloudInstanceID should be the number in the URL after -->
        <!-- "https://app.activecollab.com/" when you log in to Active Collab. -->
        <add key="ActiveCollabCloudInstanceID" value="123456"/>
        
        <!-- The ActiveCollabApiKey can be generated by the KeyGenerator project -->
        <!-- in the C# Active Collab Feather SDK -->
        <!-- (https://github.com/sfarbota/c-sharp-activecollab-feather-sdk). -->
        <add key="ActiveCollabApiKey" value="YOUR-API-KEY" />
        
        <!-- The ActiveCollabProjectName must be the name of a project -->
        <!-- that already exists in your Active Collab instance. -->
        <add key="ActiveCollabProjectName" value="Your Active Collab Project Name" />

        <!-- The TracMySqlConnectionString must point to a Trac database -->
        <!-- that already exists for your Trac instance. -->
        <add key="TracMySqlConnectionString" value="SERVER=YOURSERVERNAME;PORT=3306;DATABASE=your_database_name;UID=your_db_username;PASSWORD=your_db_password" />
        
        <!-- The following are valid values for TracFieldForActiveCollabTaskGrouping: -->
        <!-- "Type", "Component", "Severity", "Priority", "Owner", "Reporter", "Milestone", "Status", -->
        <!-- the name of a custom ticket field in your Trac database, -->
        <!-- or blank for no task grouping in Active Collab. -->
        <add key="TracFieldForActiveCollabTaskGrouping" value="" />
    </appSettings>
<system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" 
           invariant="MySql.Data.MySqlClient" 
           description=".Net Framework Data Provider for MySQL"
           type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.7.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-7.0.0.0" newVersion="7.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>